import java.util.Scanner;

public class Point {
    private long x, y;
    public static long ucln(long a, long b){
        if(b == 0) return a;
        return ucln(b, a%b);
    } 
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t > 0){
            int n = sc.nextInt();
            long res = 1;
            for(int i = 2; i <= n; i++){
                long x = ucln(res, i);
                res = res * i / x;
            }
            System.out.println(res);
            t--;
        }
    }
}
